# OpenCode Zen Configuration (FREE!)
# Get your API key from: https://opencode.ai/auth
ZEN_BASE_URL=https://opencode.ai/zen/v1
ZEN_API_KEY=your-zen-api-key-here

# Default Model Configuration (Free Zen models)
# Available: grok-code, code-supernova
DEFAULT_MODEL=grok-code

# API Configuration
PORT=3010
# Comma-separated list of allowed origins for CORS (e.g., http://localhost:3011,https://myapp.com)
# Use '*' to allow all origins (default if not set)
ALLOWED_ORIGINS=http://localhost:3011

# Development Mode (disable rate limiting for development)
# Set to 'true' for unlimited requests during development
DEV_MODE=false

# Rate Limiting (only applies when DEV_MODE=false)
# Window in milliseconds (900000 = 15 minutes)
RATE_LIMIT_WINDOW_MS=900000
# Max requests per window
RATE_LIMIT_MAX_REQUESTS=100

# Multiple Provider Support (comma-separated API keys for multiple accounts)
GROQ_API_KEYS=your-groq-key-1,your-groq-key-2,your-groq-key-3
NVIDIA_API_KEYS=your-nvidia-key-1,your-nvidia-key-2
GEMINI_API_KEYS=your-gemini-key-1,your-gemini-key-2,your-gemini-key-3
OPENROUTER_API_KEYS=your-openrouter-key-1,your-openrouter-key-2
TOGETHER_API_KEYS=your-together-key-1,your-together-key-2
FIREWORKS_API_KEYS=your-fireworks-key-1,your-fireworks-key-2
CEREBRAS_API_KEYS=your-cerebras-key-1,your-cerebras-key-2,your-cerebras-key-3
ANTHROPIC_API_KEYS=your-anthropic-key-1,your-anthropic-key-2
DEEPSEEK_API_KEYS=your-deepseek-key-1,your-deepseek-key-2
MISTRAL_API_KEYS=your-mistral-key-1,your-mistral-key-2
COHERE_API_KEYS=your-cohere-key-1,your-cohere-key-2

# Provider Base URLs
GROQ_BASE_URL=https://api.groq.com/openai/v1
NVIDIA_BASE_URL=https://integrate.api.nvidia.com/v1
GEMINI_BASE_URL=https://generativelanguage.googleapis.com
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
TOGETHER_BASE_URL=https://api.together.xyz/v1
FIREWORKS_BASE_URL=https://api.fireworks.ai/inference/v1
CEREBRAS_BASE_URL=https://api.cerebras.ai/v1
ANTHROPIC_BASE_URL=https://api.anthropic.com/v1
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
MISTRAL_BASE_URL=https://api.mistral.ai/v1
COHERE_BASE_URL=https://api.cohere.ai/v1

# Provider Priority (comma-separated, opencode as final fallback)
PROVIDER_PRIORITY=cerebras,groq,nvidia,gemini,openrouter,together,fireworks,anthropic,deepseek,mistral,cohere,opencode

# Performance Tuning
SPEED_WEIGHT_MULTIPLIER=0.4
ERROR_PENALTY_MULTIPLIER=2.0
HEALTH_CHECK_INTERVAL=30000
STATS_UPDATE_INTERVAL=5000
MAX_STATS_HISTORY=1000

# Stats File Configuration
PROVIDER_STATS_FILE=./provider_stats.json

# URLs to ping periodically (comma-separated)
PING_URLS=https://example.com,https://another.com
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
# EMAIL_SECURE=true
EMAIL_USER=
EMAIL_PASS=
OWNER_MAIL=
# Admin Credentials
ADMIN_USER=
ADMIN_PASS=
ALLOWED_ORIGINS=http://localhost:3011